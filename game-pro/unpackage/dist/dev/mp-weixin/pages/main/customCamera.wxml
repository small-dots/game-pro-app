<view class="contents"><view style="position:relative;width:100%;height:750rpx;"><camera style="position:absolute;top:0;left:0;width:100%;height:750rpx;" device-position="{{device}}" flash="{{flash}}" data-event-opts="{{[['error',[['error',['$event']]]]]}}" binderror="__e"><cover-view hidden="{{!(syShow)}}" class="textInfo"><cover-view class="textInfoTile">示例Demo</cover-view><cover-view class="topItem">{{"拍摄时间："+nowTime}}</cover-view><cover-view class="topItem">{{"天气："+weather+"℃"}}</cover-view><cover-view class="topItem">{{"拍摄人："+userName}}</cover-view><cover-view class="topItem">{{"地点："+address}}</cover-view></cover-view><cover-image class="xzImg" src="https://cdn.zhoukaiwen.com/xz.png" data-event-opts="{{[['tap',[['xzBtn',['$event']]]]]}}" bindtap="__e"></cover-image><cover-image class="sgdImg" src="{{sgdUrl}}" data-event-opts="{{[['tap',[['sgdBtn',['$event']]]]]}}" bindtap="__e"></cover-image></camera></view><view class="syBtns"><button data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e">带水印</button><button data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" bindtap="__e">无水印</button></view><view data-event-opts="{{[['tap',[['takePhoto',['$event']]]]]}}" class="cameraBtn" bindtap="__e"><view class="cameraBtn2"></view></view><view class="bottomBtn"><view data-event-opts="{{[['tap',[['goBack',['$event']]]]]}}" class="btn" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['goAlbum',['$event']]]]]}}" class="btn" bindtap="__e">相册</view></view></view>